  "scripts": {
    "start": "webpack-dev-server",
    "start-aot": "npm run ngc && webpack-dev-server --config webpack.aot.js",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "build": "rimraf dist && webpack --progress --config webpack.prod.js",
