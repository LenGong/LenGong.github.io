{
    "compilerOptions": {
        "target": "es5",
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noEmitHelpers": false,
        "noEmitOnError": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true
    }
}
